{
  "functions": {
    "api/[...proxy].js": {
      "runtime": "nodejs18.x",
      "memory": 128,
      "maxDuration": 10
    }
  },
  "rewrites": [
    { "source": "/corsdemo", "destination": "/api/corsdemo" },


    { "source": "/https:/(.*)", "destination": "/api/https://$1" },
    { "source": "/http:/(.*)", "destination": "/api/http://$1" },


    { "source": "/api/(.*)", "destination": "/api/$1" },


    { "source": "/api", "destination": "/api" }
  ]
}
