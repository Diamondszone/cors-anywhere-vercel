{
  "routes": [
    {
      "src": "^/iscorsneeded$",
      "dest": "/api/proxy"
    },
    {
      "src": "^/api/proxy$",
      "dest": "/api/proxy"
    },
    {
      "src": "^(/https://.*)$",
      "dest": "/api/proxy"
    },
    {
      "src": "^(/http://.*)$", 
      "dest": "/api/proxy"
    },
    {
      "src": "^(/.*)$",
      "dest": "/api/proxy"
    }
  ]
}
