{
  "functions": {
    "api/[...proxy].js": {
      "runtime": "nodejs18.x",
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "^/corsdemo$", "dest": "/api/corsdemo" },

    { "src": "^/(https?:\\/\\/.*)$", "dest": "/api/$1" },

    { "src": "^/api/(.*)$", "dest": "/api/$1" },

    { "src": "^/api$", "dest": "/api" }
  ]
}
